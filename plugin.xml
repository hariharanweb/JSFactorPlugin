<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="hari.jsfactor.ui.JSFactor"
            contributorClass="org.eclipse.ui.texteditor.BasicTextEditorActionContributor"
            default="true"
            extensions="js"
            id="hari.jsfactor.ui.JSFactor"
            name="JSFactor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.views">
   </extension>
   
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="hari.jsfactor.ui.action.RenameAction"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+F2">
      </key>
   </extension>
   
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="hari.jsfactor.ui.action.RenameAction"
            name="RenameAction">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="hari.jsfactor.ui.action.RenameAction"
            commandId="hari.jsfactor.ui.action.RenameAction">
      </handler>
   </extension>
   
   <extension
         point="org.eclipse.ui.menus">
		<menuContribution
            locationURI="menu:org.eclipse.ui.main.menu?after=additions">
            <menu
               label="JSFactor"               
               id="hari.jsfactor.ui.menus.jsfactor">
	            <command
	                  commandId="hari.jsfactor.ui.action.RenameAction"	                  
	                  id="hari.jsfactor.ui.menu.action.RenameAction">
	            </command>
         	</menu>
		</menuContribution>
   </extension>

</plugin>
